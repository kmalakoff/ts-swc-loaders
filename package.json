{
  "name": "ts-swc-loaders",
  "version": "1.11.5",
  "description": "Typescript loaders for swc",
  "keywords": [
    "typescript",
    "swc",
    "loader",
    "require"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-loaders",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-swc-loaders.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.esm.mjs",
      "require": "./dist/cjs/index.cjs.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs.cjs",
  "module": "dist/dist/esm/index.esm.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "ts-swc": "bin/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "nvu engines tsds test:node --no-timeouts test/unit/*.test.*",
    "test:engines": "npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.1.0",
    "ends-with": "^1.0.1",
    "exit": "^0.1.2",
    "get-tsconfig-compat": "^2.0.3",
    "homedir-polyfill": "^1.0.3",
    "is-builtin-module": "^3.2.1",
    "mkdirp-classic": "^0.5.3",
    "module-root-sync": "^1.1.1",
    "pirates": "^2.1.2",
    "resolve-bin-sync": "^0.1.4",
    "rimraf2": "^2.8.2",
    "short-hash": "^1.0.0",
    "starts-with": "^1.0.2",
    "ts-swc-transform": "^1.13.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/exit": "^0.1.33",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "core-js": "^3.39.0",
    "cr": "^0.1.0",
    "getopts-compat": "^2.2.5",
    "module-link-unlink": "^0.1.12",
    "react": "^19.0.0",
    "tsds-mocha": "^1.12.9"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.esm.ts"
  }
}
