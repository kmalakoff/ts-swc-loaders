{
  "name": "ts-swc-loaders",
  "version": "0.0.0",
  "description": "Typescript loaders for swc",
  "keywords": [
    "typescript",
    "swc",
    "loader",
    "require"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-loaders",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-swc-loaders.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "tsds link && mocha-compat test/**/*.test.js --no-timeouts && tsds unlink",
    "test:engines": "nvu engines npm test"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^5.32.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "lodash.flattendeep": "^4.4.0",
    "mocha": "^10.0.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1",
    "ts-dev-stack": "^0.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "@swc/core": "^1.2.222",
    "core-js": "^3.24.1",
    "cross-spawn-cb": "^0.6.8",
    "just-extend": "^6.0.1",
    "mkdirp": "^0.5.6",
    "node-version-call": "^0.3.4",
    "os-shim": "^0.1.3",
    "pirates": "^2.1.2",
    "regenerator-runtime": "^0.13.9",
    "short-hash": "^1.0.0",
    "sort-package-json": "^1.57.0",
    "ts-constants": "^0.1.0",
    "ts-node": "10.8.2",
    "typescript": "^4.7.4"
  }
}
