{
  "name": "ts-swc-loaders",
  "version": "0.3.4",
  "description": "Typescript loaders for swc",
  "keywords": [
    "typescript",
    "swc",
    "loader",
    "require"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-loaders",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-swc-loaders.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.esm.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/dist/esm/index.esm.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds link && mocha-compat test/spec/**/*.test.js --no-timeouts && nvu latest mocha test/spec/**/*.test.mjs --no-timeouts && tsds unlink",
    "test:engines": "tsds link && nvu engines mocha-compat test/spec/**/*.test.js --no-timeouts && nvu 12,14,16,18,latest mocha test/spec/**/*.test.mjs --no-timeouts && tsds unlink",
    "version": "tsds version"
  },
  "dependencies": {
    "@swc/core": "^1.4.1",
    "core-js": "^3.36.0",
    "get-tsconfig-compat": "^0.1.0",
    "minimatch": "^3.1.2",
    "mkdirp": "^0.5.6",
    "node-version-call": "^0.4.7",
    "os-shim": "^0.1.3",
    "path-posix": "^1.0.0",
    "pirates": "^2.1.2",
    "regenerator-runtime": "^0.14.1",
    "short-hash": "^1.0.0",
    "slash": "^1.0.0",
    "ts-node": "10.8.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "cross-spawn-cb": "^0.6.18",
    "depcheck": "^1.4.7",
    "mocha": "^10.3.0",
    "react": "^18.2.0",
    "ts-dev-stack": "^0.12.4"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
