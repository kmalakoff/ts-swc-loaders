{
  "name": "ts-swc-loaders",
  "version": "1.7.2",
  "description": "Typescript loaders for swc",
  "keywords": [
    "typescript",
    "swc",
    "loader",
    "require"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-loaders",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-swc-loaders.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.esm.mjs",
      "require": "./dist/cjs/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/dist/esm/index.esm.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "ts-swc": "bin/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts test/unit/*.test.*",
    "test:engines": "nvu engines tsds test:node --no-timeouts test/unit/*.test.*",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^1.5.7",
    "ends-with": "^1.0.1",
    "exit": "^0.1.2",
    "get-tsconfig-compat": "^2.0.2",
    "is-builtin-module": "^3.2.1",
    "mkdirp-classic": "^0.5.3",
    "module-root-sync": "^0.1.1",
    "module-which": "^0.2.1",
    "os-shim": "^0.1.3",
    "pirates": "^2.1.2",
    "regenerator-runtime": "^0.14.1",
    "short-hash": "^1.0.0",
    "starts-with": "^1.0.2",
    "ts-swc-transform": "^1.9.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/exit": "^0.1.33",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "core-js": "^3.39.0",
    "cr": "^0.1.0",
    "getopts-compat": "^2.2.5",
    "mocha-compat": "^3.6.2",
    "module-link-unlink": "^0.1.3",
    "react": "^19.0.0",
    "resolve": "^1.22.10",
    "ts-swc-loaders": "^1.7.1",
    "tsds-mocha": "^1.8.12"
  },
  "engines": {
    "node": ">=12"
  },
  "tsds": {
    "source": "src/index.esm.ts"
  }
}
