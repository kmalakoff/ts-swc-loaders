{
  "name": "ts-swc-loaders",
  "version": "2.0.18",
  "description": "Typescript loaders for swc",
  "keywords": [
    "typescript",
    "swc",
    "loader",
    "require"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-loaders",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-swc-loaders.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index-esm.js",
      "require": "./dist/cjs/index-cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index-cjs.js",
  "types": "dist/cjs/index-cjs.d.cts",
  "bin": {
    "ts-swc": "bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "test": "mocha --no-timeouts test/**/*.test.*",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.2.10",
    "ends-with": "^1.0.1",
    "exit": "^0.1.2",
    "get-tsconfig-compat": "^2.0.3",
    "homedir-polyfill": "^1.0.3",
    "is-builtin-module": "^3.2.1",
    "mkdirp-classic": "^0.5.3",
    "module-root-sync": "^1.1.13",
    "pirates": "^2.1.2",
    "resolve-bin-sync": "^0.1.16",
    "rimraf2": "^2.8.2",
    "short-hash": "^1.0.0",
    "starts-with": "^1.0.2",
    "ts-swc-transform": "^2.0.20"
  },
  "devDependencies": {
    "@types/exit": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "core-js": "*",
    "cr": "*",
    "getopts-compat": "*",
    "module-link-unlink": "*",
    "react": "*",
    "tsds-mocha": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index-esm.ts"
  }
}
